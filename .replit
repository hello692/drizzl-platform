run = "npm run dev"
entrypoint = "index.js"
hidden = [".config", "package-lock.json", ".next", ".swc"]
modules = ["nodejs-22", "postgresql-16", "python-3.11"]

[nix]
channel = "stable-24_11"
packages = ["freetype", "lcms2", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "oneDNN", "openjpeg", "re2", "tcl", "tk", "unzip", "zlib"]

[env]
NEXT_TELEMETRY_DISABLED = "1"

[deployment]
build = ["npm", "run", "build"]
run = ["npm", "run", "start"]
deploymentTarget = "cloudrun"

[[ports]]
localPort = 3000

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 36745
externalPort = 3000

[[ports]]
localPort = 38147
externalPort = 4200

[[ports]]
localPort = 39819
externalPort = 3003

[[ports]]
localPort = 42203
externalPort = 3001

[[ports]]
localPort = 42959
externalPort = 3002

[[ports]]
localPort = 42989
externalPort = 5173

[[ports]]
localPort = 46283
externalPort = 5000

[agent]
expertMode = true
integrations = ["google-calendar:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Next.js Dev Server"

[[workflows.workflow]]
name = "Next.js Dev Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"
